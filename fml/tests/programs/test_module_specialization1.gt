type List(@T) = struct
    dummy_data: @T,
end


type Idx(@Self, @Output) = struct
    idx: fn(@Self, I32) @Output,
end

-- Can we make an instance for a specialization of List(T)?
const IntListIdx = Idx({
    .idx = fn(self List(I32), i I32) I32 = self$.dummy_data end
})

